age=int(input("Enter your age:"))
print(f"My name is:{age}")
print(type(age))

salary=float(input("enter your salary"))
print(f"Salary is:{salary}")

married=bool(input("are you married or not?"))
print(f"Marrige status:{married}")

hobbies=list(input("enter your hobbies"))
print(f"Hobbies are:{hobbies}")

family=tuple(input("enter your family members name"))
print(f"Family members are:{family}")

grades=set(input("enter your each sem grade"))
print(f"Grades:{grades}")







