cities=input("enter different cities")
print(f"Cities are:{cities}")
print(type(cities))


name=input("Enter your name")
print(f"Name:{name}")
print(type(name))

age=int(input("Enter your age:"))
print(f"My name is:{age}")
print(type(age))

degree=input("Enter your currently persuing degree")
print(f"You are persuing:{degree}")
print(type(degree))

hobbies=(input("Enter your hobbies")).split()
print(f"Your hobbies are:{hobbies}")
print(type(hobbies))

ambition=input("What is your ambition")
print(f"Your ambition is:{ambition}")
print(type(ambition))
